# Define toolbox directory if not set
if [ -z "$TOOLBOX" ]; then
    TOOLBOX="$( cd "$( dirname "${BASH_SOURCE[0]}" )/../.." && pwd )"
fi

# Source helpers
. $TOOLBOX/lib/header
. $TOOLBOX/lib/utils
. $TOOLBOX/lib/brew
. $TOOLBOX/lib/completion
. $TOOLBOX/lib/dnsmasq
. $TOOLBOX/lib/gem
. $TOOLBOX/lib/git
. $TOOLBOX/lib/httpd
. $TOOLBOX/lib/macos
. $TOOLBOX/lib/mysql
. $TOOLBOX/lib/node
. $TOOLBOX/lib/php
. $TOOLBOX/lib/pip
. $TOOLBOX/lib/preferences

e_header "All done!"

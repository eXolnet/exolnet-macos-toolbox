# Define eXolnet directory if not set
if [ -z "$EXOLNET" ]; then
    EXOLNET="$( cd "$( dirname "${BASH_SOURCE[0]}" )/../.." && pwd )"
fi

# Source helpers
. $EXOLNET/lib/header
. $EXOLNET/lib/utils
. $EXOLNET/lib/brew
. $EXOLNET/lib/mysql
. $EXOLNET/lib/httpd
. $EXOLNET/lib/php
. $EXOLNET/lib/node
. $EXOLNET/lib/completion

e_header "All done!"
